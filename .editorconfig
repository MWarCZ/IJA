root = true

[*]
end_of_line = LF
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = true

[*.java]
indent_size = 4
indent_style = space

[*fxml]
indent_size = 4
indent_style = tab

[*.xml]
indent_style = tab
indent_size = 4

[Makefile]
indent_style = tab
indent_size = 4

[*.bat]
end_of_line = CRLF
indent_style = tab
indent_size = 4
