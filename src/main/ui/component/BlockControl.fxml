        <RowConstraints minHeight="0.0" percentHeight="25.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
   <children>
       <Button focusTraversable="false" fx:id="centerButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Center" GridPane.columnIndex="1" GridPane.rowIndex="0" GridPane.rowSpan="3">
         <contextMenu>
            <ContextMenu>
              <items>
                  <MenuItem fx:id="menu_up" mnemonicParsing="false" text="Up" />
                  <MenuItem fx:id="menu_revup" mnemonicParsing="false" text="RevUp" />
                  <MenuItem fx:id="menu_revdown" mnemonicParsing="false" text="RevDown" />
                  <MenuItem fx:id="menu_down" mnemonicParsing="false" text="Down" />
